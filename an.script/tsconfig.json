{
   "compilerOptions": {
    "target": "ES5",
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist/",
    "sourceRoot": "src",
    "rootDir": "src",
    "sourceMap": true,
    "types": [
      "node"
    ]
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "include": [
    "./src/*",
    "./src/*/*",
    "./src/*/*/*"
  ],
  "exclude": [
    "./node_modules"
  ]
}